generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Text {
  id     Int    @id @default(autoincrement())
  textTr String @db.VarChar(255)
  textEn String @db.VarChar(255)
  textRu String @db.VarChar(255)
  textDe String @db.VarChar(255)
}
